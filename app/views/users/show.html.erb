<p>
    <%= link_to "Log out", logout_path, method: :delete %>
</p>
<p>
    <%= link_to "New contact", new_contact_path %>
</p>
<ol>
    <% @contacts.each do |contact| %>
        <%= link_to(contact_path(contact)) do %>
            <p>
                <%= contact.first_name %>
            </p>
            <p>
                <%= contact.last_name %>
            </p>
            <p>
                <%= contact.email %>
            </p>
            <p>
                <%= contact.phone_number %>
            </p>
        <% end %>
        <hr>
    <% end %>
</ol>
